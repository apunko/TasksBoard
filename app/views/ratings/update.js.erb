$('#star').raty({
  readOnly: true,
  score: <%= @task.average_rating(@task.id) %>,
  path: '/assets'
});
//
$('#user_star').raty({
  score: <%= @rating.score %>,
  path: '/assets',
  click: function(score, evt) {
    $.ajax({
      url: '/ratings/' + <%= @rating.id %>,
      type: 'PATCH',
      data: { score: score }
    }).done(function (data){});
  }
});